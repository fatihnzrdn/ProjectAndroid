<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignIn">

    <View
        android:id="@+id/signInBackground"
        android:layout_width="367dp"
        android:layout_height="281dp"
        android:background="@drawable/rounded_corner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.131" />

    <View
        android:id="@+id/signInBackground3"
        android:layout_width="380dp"
        android:layout_height="303dp"
        android:background="@drawable/rounded_corner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.777" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sign Up"
        app:layout_constraintBottom_toBottomOf="@+id/signInBackground"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground"
        app:layout_constraintStart_toStartOf="@+id/signInBackground"
        app:layout_constraintTop_toBottomOf="@+id/passwordText" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sign In"
        app:layout_constraintBottom_toBottomOf="@+id/signInBackground3"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground3"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/signInBackground3"
        app:layout_constraintTop_toBottomOf="@+id/passwordText2" />

    <EditText
        android:id="@+id/emailText"
        android:layout_width="331dp"
        android:layout_height="49dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="EMAIL"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toTopOf="@+id/passwordText"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground"
        app:layout_constraintStart_toStartOf="@+id/signInBackground"
        app:layout_constraintTop_toTopOf="@+id/signInBackground"
        app:layout_constraintVertical_bias="0.845" />

    <EditText
        android:id="@+id/emailText2"
        android:layout_width="331dp"
        android:layout_height="49dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="EMAIL"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/signInBackground3"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground3"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/signInBackground3"
        app:layout_constraintTop_toTopOf="@+id/signInBackground3"
        app:layout_constraintVertical_bias="0.311" />

    <EditText
        android:id="@+id/passwordText"
        android:layout_width="331dp"
        android:layout_height="49dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="PASSWORD"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/signInBackground"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground"
        app:layout_constraintStart_toStartOf="@+id/signInBackground"
        app:layout_constraintTop_toTopOf="@+id/signInBackground"
        app:layout_constraintVertical_bias="0.629" />

    <EditText
        android:id="@+id/passwordText2"
        android:layout_width="331dp"
        android:layout_height="49dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="PASSWORD"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/signInBackground3"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground3"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/signInBackground3"
        app:layout_constraintTop_toTopOf="@+id/signInBackground3"
        app:layout_constraintVertical_bias="0.594" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="116dp"
        android:layout_height="36dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:text="LOGIN"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/emailText2"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground3"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/signInBackground3"
        app:layout_constraintTop_toTopOf="@+id/signInBackground3" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sign up"
        android:textSize="24dp"
        app:layout_constraintBottom_toTopOf="@+id/emailText"
        app:layout_constraintEnd_toEndOf="@+id/signInBackground"
        app:layout_constraintStart_toStartOf="@+id/signInBackground"
        app:layout_constraintTop_toTopOf="@+id/signInBackground" />

</androidx.constraintlayout.widget.ConstraintLayout>